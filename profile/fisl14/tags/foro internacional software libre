        <script type="text/javascript">
  // Reestrutura datats de posts na visualização de blog
  if ( jQuery('html').attr('lang') == 'en' )
    jQuery('.blog-post .date').html(function(pos, txt){
      return txt.replace(
        /^\s*([^ ]{3}).* ([0-9]+), (.+)/,
        '<strong>$2</strong> <span>$1</span> <b>$3</b>'
      );
    });
  if ( jQuery('html').attr('lang') == 'pt' )
    jQuery('.blog-post .date').html(function(pos, txt){
      return txt.replace(
        /([0-9]+) ([^\s]+) ([^\s]{3}).* [^\s]+ ([0-9]+)/,
        '<strong>$1</strong> <span>$2 $3</span> <b>$4</b>'
      );
    });
  // Remove a virgula antes do autor:
  jQuery('.blog-post .author').html(function(pos, txt){
    return txt.replace( /^\s*,/, '' );
  });
  // Move as tags para depois do texto
  jQuery('#article-tags').insertAfter('#article .article-body');
</script>

<script type="text/javascript">
  jQuery('.members-block .profile_link .fn').each(function(pos, el) {
    el.parentNode.title = el.innerHTML
  });
  jQuery('.highlights-block-pager').html(''); //Needed to avoid duplication of links
  if (!jQuery('body').hasClass('action-profile_design-index')) {
    jQuery('.highlights-block').prependTo('#boxes');
  }
  jQuery('.highlights-block .highlights-label').each(function(n, el) {
    var label = jQuery(el);
    if ( label.text() == '' ) return label.remove();
    labelPieces = label.text().split('|');
    label.empty();
    if ( labelPieces.length == 1 ) {
      jQuery('<span></span>').appendTo(label).text(labelPieces[0]);
    } else {
      jQuery('<strong></strong>').appendTo(label).text(labelPieces[0]);
      for ( var i=1; i<labelPieces.length; i++ ) {
        jQuery('<span></span>').appendTo(label).text(labelPieces[i]);
      }
    }
  });
</script>


