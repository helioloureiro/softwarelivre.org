        monthNamesShort: ["Jan","Fev","Mar","Abr","Maio","Jun","Jul","Ago","Set","Out","Nov","Dez"],
        navigationAsDateFormat: false,
        nextText: "Próximo",
        numberOfMonths: 1,
        prevText: "Anterior",
        selectOtherMonths: false,
        shortYearCutoff: "+10",
        showButtonPanel: false,
        showCurrentAtPos: 0,
        showMonthAfterYear: false,
        showOn: "focus",
        showOptions: {},
        showOtherMonths: false,
