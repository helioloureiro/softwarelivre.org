      </div>
  </div>
  <p class="highlights-block-footer"></p>
  <script type="text/javascript">
    (function($) {
     $.fn.cycle.transitions.customFade = function($cont, $slides, opts) {
       $slides.not(":eq(" + opts.currSlide + ")").css("opacity", 0);
       opts.before.push(function(curr, next, opts) {
         $.fn.cycle.commonReset(curr, next, opts);
         opts.cssBefore.opacity = 0;
       });
       opts.animIn = {
         opacity: 1
       };
       opts.animOut = {
         opacity: 0
       };
       opts.cssBefore = {
         top: 0,
         left: 0,
         zIndex: 1,
         display: 'block'
       };
       opts.cssAfter = {
         zIndex: 0
       };
     };
     var options = {
       fx: 'customFade',
       fastOnEvent: 1,
       timeout: 5000
     };
       options.pager = '#block-655738 .highlights-block-pager';
     $('#block-655738 .highlights-container').cycle(options);
    })(jQuery);
  </script>
