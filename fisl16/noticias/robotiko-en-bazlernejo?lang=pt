       next: '#block-674968 .gallery-block-next',
       speed: 2000,
       timeout: 5000
      }
      $('#block-674968 .gallery-list').cycle(options);
    })(jQuery);
  </script>
  <p class="gallery-block-footer"></p>
</div></div></div></div>

