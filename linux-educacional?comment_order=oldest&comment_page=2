<div class="comments-action-bar">
    <a class="comment-footer comment-footer-link comment-footer-hide comment-actions-reply button" id="comment-reply-to-28673" title="Reply" href="#" onclick="var f = add_comment_reply_form(this, 28673); f.find(&#39;comment_title, textarea&#39;).val(&#39;&#39;); return false; return false;"></a>
