<script>
(function() {
  var body = jQuery(document.body);
  function registerDocWidth() {
    var docW = body.width();
    for ( var w=600; w<=2000; w+=50 ) {
      if ( docW < w ) {
        body.removeClass('gt'+w);
        body.addClass('lt'+w);
      } else {
        body.removeClass('lt'+w);
        if ( docW > w ) body.addClass('gt'+w);
      }
    }
  }
  registerDocWidth();
  jQuery(window).resize(registerDocWidth);

  /*** Register and update Scrolledheight for CSS *****************************/
  function registerScrolling() {
    var doc = jQuery(document.documentElement);
    var scrollTop = jQuery(document).scrollTop();
    if(scrollTop<1) doc.addClass('not-scrolled').removeClass('scrolled');
    else doc.addClass('scrolled').removeClass('not-scrolled');
    for ( var i=10; i<=500; i+=10 ) {
      doc.removeClass('scrolled-gt'+i).removeClass('scrolled-lt'+i);
      if (scrollTop<i) doc.addClass('scrolled-lt'+i);
      else doc.addClass('scrolled-gt'+i);
    }
  }
  registerScrolling(); // run now and init the right layout size
  jQuery(registerScrolling); // run onload, and adjust to the scroll
  jQuery(document).bind('scroll', registerScrolling); // run when page scroll
})();
</script>

<script>
  // Esconde os botões da comunidade
  jQuery(function($){ $('.profile-info-options').hide()});
</script>

<script>
  jQuery(document.body).addClass('not-logged-in');
  // renomeia o botão de entrar na comunidade
  jQuery(window).bind('userDataLoaded', function(ev, data) {
    console.log(data);
    if ( data && data.login ) {
      jQuery(document.body).removeClass('not-logged-in');
      //jQuery(document.body).addClass('logged-in');
    }
    jQuery('#link_login strong').text('Login');
    jQuery(':lang(pt) #logout strong').text('Logout');
    jQuery('#user .admin-link strong').text('Adm');
    jQuery('#user .icon-menu-ctrl-panel + strong').text('Meu Painel');
    jQuery(':lang(pt) .profile-info-options .leave-community span span').text('Abandonar a comunidade');
    // Move os botões de registrar e logar no ambiente para o bloco de membros
    jQuery('.members-signup-link').appendTo('.members-block .join-leave-button');
    jQuery('.members-login-link').appendTo('.members-block .join-leave-button');
  });
</script>


