jQuery(function($) { $('div.feed-reader-block a[href^="http://"]').filter(function() {
    return this.hostname && this.hostname !== location.hostname;
}).each(function() {
    var link = jQuery(this);
    var faviconURL =
    link.attr('href').replace(/^(http:\/\/[^\/]+).*$/, '$1') + '/favicon.ico';
    var faviconIMG = jQuery('<img src="http://softwarelivre.org/designs/icons/default/Tango/16x16/actions/media-playback-start.png" alt="favicon" />')['prependTo'](link);
    var extImg = new Image();
    extImg.src = faviconURL;
    if (extImg.complete) faviconIMG.attr('src', faviconURL);
    else extImg.onload = function() {
        faviconIMG.attr('src', faviconURL);
    }; });
});
</script>

