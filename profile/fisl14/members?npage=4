<script>
  function registerDocWidth() {
    var body = jQuery(document.body);
    var docW = jQuery(document).width();
    for ( var w=600; w<=2000; w+=50 ) {
      if ( docW < w ) {
        body.removeClass('gt'+w);
        body.addClass('lt'+w);
      } else {
        body.removeClass('lt'+w);
        if ( docW > w ) body.addClass('gt'+w);
      }
    }
  }
  registerDocWidth();
  jQuery(window).resize(registerDocWidth);
</script>

<script>
  // Move os botões da comunidade para o menu de usuário
  jQuery(function($){ $('.profile-info-options').insertBefore('#user > form') });
</script>

<script>
  // renomeia o botão de entrar na comunidade
  jQuery(window).bind('userDataLoaded', function() {
    jQuery('.profile-info-options .icon-add span').text('Eu vou!');
    jQuery('#link_login strong').text('Login');
    jQuery(':lang(pt) #logout strong').text('Logout');
    jQuery('#user .admin-link strong').text('Adm');
    jQuery('#user .icon-menu-ctrl-panel + strong').text('Meu Painel');
    jQuery(':lang(pt) .profile-info-options .leave-community span span').text('Abandonar a comunidade');
    // Move o botão de entrar na comunidade para o bloco de membros
    jQuery('.profile-info-options .icon-add').insertBefore('.members-block .block-footer-content a');
  });
</script>

<div id="toTop">Ir ao Topo</div>
<script>
  jQuery(window).scroll(function() {
	  if(jQuery(this).scrollTop() != 0) jQuery(document.body).addClass('scrolled');
	  else jQuery(document.body).removeClass('scrolled');
  });
  jQuery('#toTop').click(function() {
	  jQuery('body,html').animate({scrollTop:0},800);
  });
</script>

