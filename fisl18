//Adição de classe na navegação ao rolar a página
	$("document").ready(function($) {
		var nav = $('aside');
		$(window).scroll(function() {
			if ($(this).scrollTop() > 60) {
				nav.addClass("fix-top");
			} else {
				nav.removeClass("fix-top");
			}
		});
	});




