<div class="comments-action-bar">
    <a class="comment-footer comment-footer-link comment-footer-hide comment-actions-reply button" id="comment-reply-to-49220" title="Responder" href="#" onclick="var f = add_comment_reply_form(this, 49220); f.find(&#39;comment_title, textarea&#39;).val(&#39;&#39;); return false; return false;"></a>
