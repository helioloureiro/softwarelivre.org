    (function($) {
     var options = {fx: 'fade', pause: 1, fastOnEvent: 1, timeout: 2000};
       options.prev = '#block-115785 .icon-media-prev';
       options.next = '#block-115785 .icon-media-next';
      $('#block-115785 .slideshow-container').cycle(options);
    })(jQuery);

    function togglePlayback(slideshow, button) {
      var $ = jQuery;
      if (button.className == 'icon-media-pause') {
        button.className = 'icon-media-play';
        $(slideshow).cycle('pause');
      } else {
        button.className = 'icon-media-pause';
        $(slideshow).cycle('resume');
      }
    }
