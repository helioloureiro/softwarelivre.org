</div></div></div><div id="profile-footer"></div>
            <br style='clear: both'/>
          </div><!-- end id="content-inner" -->
        </div><!-- end id="content" -->
      </div><!-- end id="wrap-2" -->
    </div><!-- end id="wrap-1" -->
    
    <div id="theme-footer">
      <script type="text/javascript">
  // Move search and user menu to top bar:
//  jQuery('#top-search').appendTo('#top-menu');
  jQuery('#fisl-languages').appendTo('#top-menu-inner');
  jQuery('#user').appendTo('#top-menu');
  // Identify "View all" link in members block.
  jQuery('.members-block a[href="/profile/fisl15/members"]').addClass('view-all');
  // Set profile name as title:
  jQuery('.members-block .profile_link .fn').each(function(pos, el) {
    el.parentNode.title = el.innerHTML
  });
  jQuery('.highlights-block-pager').html(''); //Needed to avoid duplication of links
  jQuery('.highlights-block').prependTo('#boxes');
  jQuery('.highlights-block .highlights-label').each(function(n, el) {
    var label = jQuery(el);
    if ( label.text() == '' ) return label.remove();
    labelPieces = label.text().split('|');
    label.empty();
    if ( labelPieces.length == 1 ) {
      jQuery('<span></span>').appendTo(label).text(labelPieces[0]);
    } else {
      jQuery('<strong></strong>').appendTo(label).text(labelPieces[0]);
      for ( var i=1; i<labelPieces.length; i++ ) {
        jQuery('<span></span>').appendTo(label).text(labelPieces[i]);
      }
    }
  });
  /* Changed visualization of post creation date */
  var allCreationInfo = jQuery('.blog-post .created-at');
  for ( var i=0; i<allCreationInfo.length; i++ ) {
    element = jQuery(allCreationInfo[i]);
    var author = element.find('.author');
    // Remove ", by " from author
    author.html(jQuery(author).find('a'));
    // Author on the beginning
    author.prependTo($(element));
  };
</script>

<script type="text/javascript">
  jQuery(function($) {
    $('#fisl-languages a').attr('href', function(i,v) { return '/fisl15/' + v; });
    $('#fisl-languages .flag-eo').attr('href', 'http://softwarelivre.org/fisl15/programacao/invititaj-prelegantoj?lang=eo');
    $('#fisl-languages .flag-es').attr('href', 'http://softwarelivre.org/fisl15/programacao/expositores-invitados?lang=es');
    $('#fisl-languages .flag-pt').attr('href', 'http://softwarelivre.org/fisl15/programacao/palestrantes-convidados?lang=pt');
  });
</script>

