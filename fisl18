jQuery(document).ready(function($) {
            var alterClass = function() {
                var ww = document.body.clientWidth;
                if (ww < 992) {
                    $('.navbar.navbar-fisl').removeClass('sticky-top');
                } else if (ww >= 991) {
                    $('.navbar.navbar-fisl').addClass('sticky-top');
                };
            };
            $(window).resize(function() {
                alterClass();
            });
            //Fire it when the page first loads:
            alterClass();
});

// Calcular altura da base e passar para container

$(document).ready(exe);
$(window).resize(exe);

function exe(){
    var topHeight = $('.footerH').outerHeight(true);
    $('.tudo').css("min-height", "calc(100vh - "+ topHeight +"px)")
}


//Criar container para carousel
	//owl carroseu
	$(document).ready(function() {
		$(".owl-carousel").owlCarousel({
			loop: true,
			margin: 30,
			autoplay: true,
            dots:false,
			autoplayTimeout: 4000,
			touchDrag: true,
            responsive: {
                320: {
					items: 1,
				},
				600: {
					items: 2,
				},
				980: {
					items: 3,
				},	
                1020: {
					items: 1,
			}
            }
		});
	});

	$('.owl-carousel').mouseover(function() {
		owl.trigger('stop.owl.autoplay');
	})

	$('.owl-carousel').mouseleave(function() {
		owl.trigger('play.owl.autoplay', [1000]);
	})
