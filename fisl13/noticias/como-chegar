<script src="http://maps.google.com/maps?file=api&amp;v=2&amp;sensor=true&amp;key=ABQIAAAACZGlzmHu1-BFj1FKnNqlyRSGizf7lmFvT1EyGAbyxP5ICrhpQxQBIWR0fl3f7KD88YTu-srca_t_pA" type="text/javascript"></script>


<script type="text/javascript">

	var map;
	var gdir;

	function setDirections(fromAddress, toAddress) {
		gdir.load("from: " + fromAddress + " to: " + toAddress, { "locale": "pt_BR" });
	}

	function handleErrors(){
		if (gdir.getStatus().code == G_GEO_UNKNOWN_ADDRESS) {
			 alert("Nenhuma localização geográfica foi encontrada para o endereço especificado. Talvez o endereço seja muito novo ou esteja incorreto. Por favor verifique se está digitado corretamente.");
		} else if (gdir.getStatus().code == G_GEO_SERVER_ERROR) {
			 alert("Ocorreu um erro no processamento de sua busca. Por favor tente novamente agora ou mais tarde.");
		} else if (gdir.getStatus().code == G_GEO_MISSING_QUERY) {
			 alert("Você não especificou um endereço de origem.");
		} else if (gdir.getStatus().code == G_GEO_BAD_KEY) {
			 alert("O(s) termo(s) digitado(s) não é(são) válido(s). Por favor não utilize caracteres especiais.");
		} else if (gdir.getStatus().code == G_GEO_BAD_REQUEST) {
			 alert("Ocorreu um erro na sua solicitação. Por favor tente novamente agora ou mais tarde.");
		} else if (gdir.getStatus().code == G_GEO_UNKNOWN_DIRECTIONS) {
			alert("Verifique o endereço de origem digitado.");
		} else {
			alert("Um erro desconhecido ocorreu. Provavelmente algum problema de conexão. Tente novamente agora ou mais tarde.");
		}
	}
	
	function mapSearch(){
		var f = document.mapsearch.from.value;
		var t = document.mapsearch.to.value;
		setDirections(f, t);
		document.getElementById("googlemap-directions").style.display = "block";
	}

	function load() {
		if (GBrowserIsCompatible()) {
			map = new GMap2(document.getElementById("googlemap"));
			map.setCenter(new GLatLng(-30.058787, -51.173544), 15);
			map.addControl(new GLargeMapControl());
			var mapControl = new GHierarchicalMapTypeControl();
			map.addControl(mapControl);
			gdir = new GDirections(map, document.getElementById("googlemap-directions"));
			GEvent.addListener(gdir, "error", handleErrors);
		}
	}

   jQuery(document).ready(function () {
      load();
   });
</script>

<div id="mapa-como-chegar">
