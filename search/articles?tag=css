        showOptions: {},
        showOtherMonths: false,
        showWeek: false,
        stepMonths: 1,
        weekHeader: "Sem",
        yearRange: "c-10:c+10",
        yearSuffix: ""
      }).datepicker()
    </script>
    </label></div><input type="hidden" name="periods[published_at][is_metadata]" id="periods_published_at_is_metadata" value="false" /></div>
</div>

