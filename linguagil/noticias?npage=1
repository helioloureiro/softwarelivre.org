  <div class='slideshow-border'>
    <div class='slideshow-container'>
        <a href="http://www.vivo.com.br/">
          <div style="background-image: url(/thumbnails/0023/5867/logo-vivo_thumb.jpg)" title=""></div>
        </a>
    </div>
  </div>
  <script type="text/javascript">
    (function($) {
     var options = {fx: 'fade', pause: 1, fastOnEvent: 1, timeout: 4000};
      $('#block-69615 .slideshow-container').cycle(options);
    })(jQuery);

    function togglePlayback(slideshow, button) {
      var $ = jQuery;
      if (button.className == 'icon-media-pause') {
        button.className = 'icon-media-play';
        $(slideshow).cycle('pause');
      } else {
        button.className = 'icon-media-pause';
        $(slideshow).cycle('resume');
      }
    }
  </script>

</div></div></div></div>

