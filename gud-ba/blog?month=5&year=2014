  jQuery(function($) {
    $("#site-title, #assets-menu").mouseover(function() {
      clearTimeout( window.closeAssetsTimeout );
      $("#assets-menu").slideDown(400);
    });
    $("#site-title, #assets-menu").mouseout(function() {
      window.closeAssetsTimeout =
      setTimeout( function() { $("#assets-menu").slideUp(400); }, 500 );
    });
