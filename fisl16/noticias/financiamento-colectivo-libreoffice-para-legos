    <div id="theme-footer">
      <script type="text/javascript">
  // Set profile name as title:
  jQuery('.members-block .profile_link .fn').each(function(pos, el) {
    el.parentNode.title = el.innerHTML
  });
  jQuery('.highlights-block').insertAfter('#theme-header');
  jQuery('.highlights-block .highlights-label').each(function(n, el) {
    var label = jQuery(el);
    if ( label.text() == '' ) return label.remove();
    labelPieces = label.text().split('|');
    label.empty();
    if ( labelPieces.length == 1 ) {
      jQuery('<span></span>').appendTo(label).text(labelPieces[0]);
    } else {
      jQuery('<strong></strong>').appendTo(label).text(labelPieces[0]);
      for ( var i=1; i<labelPieces.length; i++ ) {
        jQuery('<span></span>').appendTo(label).text(labelPieces[i]);
      }
    }
  });

  jQuery('#fisl-sociallinks').insertAfter('.highlights-block');
  /* Changed visualization of post creation date */
  var allCreationInfo = jQuery('#article .created-at');
  for ( var i=0; i<allCreationInfo.length; i++ ) {
    element = jQuery(allCreationInfo[i]);
    var author = element.find('.author');
    // Remove ", by " from author
    author.html(jQuery(author).find('a'));
    // Author on the beginning
    author.prependTo($(element));
    // Remove "-" from comments
    var n_comments = element.find('.comments');
    n_comments.html(jQuery(n_comments).find('a'));
  };
</script>

<script type="text/javascript">
  jQuery(function($) {
    $('#fisl-languages a').attr('href', function(i,v) { return '/fisl16/' + v; });
    $('#fisl-languages .flag-es').attr('href', 'http://softwarelivre.org/fisl16/noticias/financiamento-colectivo-libreoffice-para-legos?lang=es');
    $('#fisl-languages .flag-pt').attr('href', 'http://softwarelivre.org/fisl16/noticias/financiamento-coletivo-libreoffice-para-leigos?lang=pt');
  });
</script>

