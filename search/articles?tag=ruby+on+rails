    <div class="period" id="created-at-period"><h2>Created at</h2><div><label>From:<input type="text" name="periods[created_at][start_date]" id="datepicker-from-created-at" from_id="datepicker-from-created-at" to_id="datepicker-to-created-at" />
    <script type='text/javascript'>
      jQuery('#datepicker-from-created-at').datepicker({
        disabled: false,
        altField: "",
        altFormat: "",
        appendText: "",
        autoSize: false,
        buttonImage: "",
        buttonImageOnly: false,
